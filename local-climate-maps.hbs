<!doctype html>
<html lang='en' class="width-100 height-100">
<head>
  {{> head }}
  <meta property="fb:app_id" content="187816851587993">
  <meta property="og:url" content="/">
  <meta property="og:type" content="article">
  <meta property="og:title" content="Location data for Buncombe County, NC">
  <meta property="og:description" content="The Climate Explorer allows you to view historical and projected climate variables and assess the impacts of climate change on the things you care about">
  <meta property="og:image" content="/resources/img/og.jpg">
</head>

<body class="width-100 height-100">

  {{> secondary_header }}

  <!-- <div id="main-content-wrap">

    {{> share }}

    <div id="location-search">
      <input type="text" class="autocomplete location-mapper" autocomplete="off" placeholder="Search another City, County or Zip code">
    </div>

  </div> -->

    <div id="local-climate-maps-viewport" class="padding-horrizontal d-flex d-flex-column">

      <div id="search-row" class="padding-horizontal d-flex-row flex-justify padding-top padding-bottom-half d-flex-justify" >

        <div class="rounded-box-secondary input-outer padding-horrizontal bottom-padding default-btn-height search-box-secondary width-40">
          <input id="cards-search-input" type="text" class="location-mapper input-inner-default border-none default-btn-height width-90" autocomplete="off" placeholder="Enter county, city, or zip code">
          <span class="icon icon-search search-default float-right padding-default"></span>
        </div>

        <div id="stations-select-wrapper" class="rounded-choice-box padding-horrizontal default-btn-height top-select-wrapper d-flex-center">
          <div class="select">
            <div id="stations-select-vis" class="select-styled disabled" rel="none">Stations</div>
            <ul class="select-options" style="display: none;">
              <li rel="none" class="default-select-option">Stations</li>
              <li rel="station-1" class="default-select-option">Stations 1</li>
              <li rel="station-2" class="default-select-option">Stations 2</li>
              <li rel="station-3" class="default-select-option">Stations 3</li>
              <li rel="station-4" class="default-select-option">Stations 4</li>
            </ul>
          </div>
        </div>

        <div id="downloads-select-wrapper" class="rounded-choice-box padding-horrizontal default-btn-height top-select-wrapper d-flex-center">

          <div class="select download-select">
            <div id="downnloads-select-vis" class="select-styled" rel="none" >
              <i class="fas fa-download select-icon"></i>
              <a rel="none" href="javascript:void(0);" >Downloads</a>
            </div>
            <ul class="select-options" style="display: none;">
              <li icon="fas fa-download select-icon"  class="default-select-option">
                <a rel="none" href="javascript:void(0);" >Downloads</a>
              </li>
              <li rel="download-rightmap-image" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
                <a rel="download-rightmap-image" href="javascript:void(0);" >Download map (map in right only) image</a>
              </li>
            </ul>
          </div>
        </div>

      </div>

      <div id="info-row" class="padding-vertical width-100" >
        <div class="rounded-filters-box padding-default width-100">

          <div class="d-flex-row">

            <div id="info-text-wrapper" class="width-90">
              <i class="fas fa-map-marked-alt icon-info-box"></i>
              <span id="default-city-state" class="text-info-box" ></span>
              <span id="" class="text-info-box" > in </span>
              <span id="default-city-county" class="text-info-box"></span>
              <span id="" class="text-info-box" > - </span>
              <span id="default-chart-map-varriable" class="text-info-box" rel="tmax">Avg Daily Max Temp (°F)</span>
            </div>

            <div id="filters-toggle" class="width-10">
              <div class="select filters-toggle-select">
                <div id="filters-toggle-select-vis" class="select-styled" rel="filter"></div>
              </div>
            </div>

          </div>

          <div id="filters-row" class="d-flex-row flex-justify padding-top padding-bottom d-flex-left" >

            <div id="varriable-select-wrapper" class="rounded-choice-box padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-30">
              <div class="select varriable-select">
                <div id="varriable-select-vis" class="select-styled" rel="tmax">Avg Daily Max Temp (°F)</div>
                <ul class="select-options">
                  <div icon="fas fa-temperature-high select-icon" class="default-select-option-group">
                    <i class="fas fa-temperature-high select-icon"></i>Temperature
                  </div>
                  <li rel="tmax" class="default-select-option">Avg Daily Max Temp (°F)</li>
                  <li rel="tmin" class="default-select-option">Avg Daily Min Temp (°F)</li>
                  <li rel="days_tmax_gt_90f" class="default-select-option">Days w/ max > 90°F</li>
                  <li rel="days_tmax_gt_95f" class="default-select-option">Days w/ max > 95°F</li>
                  <li rel="days_tmax_gt_100f" class="default-select-option">Days w/ max > 100°F</li>
                  <li rel="days_tmax_gt_105f" class="default-select-option">Days w/ max > 105°F</li>
                  <li rel="days_tmax_lt_32f" class="default-select-option">Days w/ max < 32°F</li>
                  <li rel="days_tmin_lt_32f" class="default-select-option">Days w/ min < 32°F</li>
                  <li rel="days_tmin_gt_80f" class="default-select-option">Days w/ min > 80°F</li>
                  <li rel="days_tmin_gt_90f" class="default-select-option">Days w/ min > 90°F</li>
                  <div icon="fas fa-cloud-rain select-icon" class="default-select-option-group">
                    <i class="fas fa-cloud-rain select-icon"></i>Precipitation
                  </div>
                  <li rel="pcpn" class="default-select-option">Total precipitation</li>
                  <li rel="days_pcpn_gt_1in" class="default-select-option">Days w/ > 1 in Precipitation</li>
                  <li rel="days_pcpn_gt_3in" class="default-select-option">Days w/ > 3 in Precipitation</li>
                  <li rel="days_dry_days" class="default-select-option">Dry Days</li>
                  <div icon="fas fa-bacon select-icon" class="default-select-option-group">
                    <i class="fas fa-bacon select-icon"></i>Other
                  </div>
                  <li rel="cdd_65f" class="default-select-option">Cooling Degree Days</li>
                  <li rel="gdd" class="default-select-option">Growing Degree Days</li>
                  <li rel="hdd_65f" class="default-select-option">Heating Degree Days</li>
                  <li rel="gddmod" class="default-select-option">Modified Growing Degree Days</li>
                </ul>
              </div>
            </div>
            <a id="national-climate-maps-secretlink-forcharts" href="../local-climate-charts" class="d-none"></a>

            <a id="chartmap-btn-chart-link"  href="../local-climate-charts" class="d-none"></a>
            <a id="chartmap-btn-map-link"  href="../local-climate-maps" class="d-none">Map</a>
            <div id="chartmap-wrapper" class="padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20 disabled">
              <div id="btn-chart" class="btn-chart btn-default circle-box-left d-flex-center width-50" val="chart" nav="local-climate-charts" link="chartmap-btn-chart-link" sel="chartmap-select-vis">
                Chart
              </div>
              <div id="btn-chart" class="btn-map btn-default-selected circle-box-right d-flex-center width-50"  nav="local-climate-maps"  val="map" link="chartmap-btn-map-link" sel="chartmap-select-vis">
                Map
              </div>
            </div>

            <div id="chartmap-select-wrapper" class="rounded-choice-box padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20">
              <div class="select chartmap-select">
                <div id="chartmap-select-vis" class="select-styled" rel="chart">Map</div>
                <ul class="select-options">
                  <li id="chartmap-select-chart-link" rel="chart" class="default-select-option" nav="local-climate-charts" link="chartmap-btn-chart-link" >Chart</li>
                  <li id="chartmap-select-map-link" rel="map" class="default-select-option" nav="local-climate-maps" link="chartmap-btn-map-link">Map</li>
                </ul>
              </div>
            </div>

            <div id="time-wrapper" class="padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-30">
              <div id="btn-chart" class="btn-summer btn-default-selected circle-box-left d-flex-center width-50" val="summer" sel="time-select-vis">
                Summer
              </div>
              <div id="btn-chart" class="btn-fall btn-default circle-box-middle d-flex-center width-50" val="fall" sel="time-select-vis" >
                Fall
              </div>
              <div id="btn-chart" class="btn-winter btn-default circle-box-middle d-flex-center width-50" val="winter" sel="time-select-vis" >
                Winter
              </div>
              <div id="btn-chart" class="btn-spring btn-default circle-box-middle d-flex-center width-50" val="spring" sel="time-select-vis" >
                Spring
              </div>
              <div id="btn-chart" class="btn-spring btn-default circle-box-right d-flex-center width-50" val="annual" sel="time-select-vis" >
                Annual
              </div>
            </div>

            <div id="time-select-wrapper" class="rounded-choice-box padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20">
              <div class="select time-select">
                <div id="time-select-vis" class="select-styled" rel="summer">Summer</div>
                <ul class="select-options">
                  <li rel="summer" class="default-select-option">Summer</li>
                  <li rel="fall" class="default-select-option">Fall</li>
                  <li rel="winter" class="default-select-option">Winter</li>
                  <li rel="spring" class="default-select-option">Spring</li>
                  <li rel="annual" class="default-select-option">Annual</li>
                </ul>
              </div>
            </div>


          </div>



        </div>
      </div>



      <div id="map-row" class="padding-horizontal padding-top-half width-100 height-100 d-flex" >

          <div id="map-wrap" class="map-wrap width-100 height-100 ">
            <!-- <div id="chart-123" class="chart-canvas width-100 height-100" data-chart-ID="123"></div> -->
            <div id="temperature-map" class="map-element"></div>
          </div>

      </div>
      </div>
    </div>

{{> nav_footer }}
{{> footer }}

<script src="https://unpkg.com/terraformer@1.0.8/terraformer.js" integrity="sha384-+M797Pj3WZVCwMmLbOxAoaWYcKJo8NSxItmI48ytcLNeAnfn1d/IckFn31jEqrzP"
        crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/0.4.1/html2canvas.min.js" integrity="sha256-c3RzsUWg+y2XljunEQS0LqWdQ04X1D3j22fd/8JCAKw=" crossorigin="anonymous"></script>
<script src="https://unpkg.com/terraformer-arcgis-parser@1.0.5/terraformer-arcgis-parser.js"
        integrity="sha384-duFUjKTSNoxEspdJNwr83CUgRxclf0ueKJB9DU/Vbit6bfWgzvZsHW6H1JLBBXhp" crossorigin="anonymous"></script>
<script type="text/javascript" src="/resources/js/scenarioComparisonMap.js"></script>
<script type="text/javascript" src="/resources/js/ce3-ui-components.js"></script>
<script type="text/javascript" src="/resources/js/local-climate-maps.js"></script>
<script type="text/javascript" src="/resources/js/nouislider.min.js"></script>
</body>
</html>
