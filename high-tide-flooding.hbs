    <div id="info-row" class="padding-vertical width-100" >
      <div class="rounded-filters-box padding-default width-100">

        <div class="d-flex-row">

          <div id="info-text-wrapper" class="width-90">
            <i class="fas fa-water icon-info-box"></i>
            <!-- <span id="default-city-state" class="text-info-box" ></span>
            <span id="" class="text-info-box" > in </span>
            <span id="default-city-county" class="text-info-box"></span> -->
            <span id="station-info-none" class="padding-horizontal">Select a Station</span>
            <span id="station-info" class="padding-horizontal d-none">
