            <li rel="download-tidal-image" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
              <a rel="download-tidal-image" href="javascript:void(0);" >Download tidal flooding image</a>
            </li>
            <li rel="download-tidal-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
              <a rel="download-tidal-data" href="https://tidesandcurrents.noaa.gov/publications/techrpt86_PaP_of_HTFlooding.csv" >Download tidal flooding data</a>
            </li>
          </ul>
