      <span class="icon icon-search search-default float-right padding-default"></span>
    </div>


    <div id="stations-select-wrapper" class="rounded-choice-box padding-horrizontal default-btn-height top-select-wrapper d-flex-center">
      <div class="select stations-select">
        <div id="stations-select-vis" class="select-styled" rel="station-1">Stations</div>
        <ul class="select-options">
          <li rel="station-1" class="default-select-option">Stations 1</li>
          <li rel="station-2" class="default-select-option">Stations 2</li>
        </ul>
      </div>
    </div>

    <div id="downloads-select-wrapper" class="rounded-choice-box padding-horrizontal default-btn-height top-select-wrapper d-flex-center">

      <div class="select download-select">
        <div id="downnloads-select-vis" class="select-styled" rel="none" href="javascript:void(0);">
          <i class="fas fa-download select-icon"></i>
          <a rel="none" href="javascript:void(0);" >Downloads</a>
        </div>
        <ul class="select-options" style="display: none;">
          <li icon="fas fa-download select-icon"  class="default-select-option">
            <a rel="none" href="javascript:void(0);" >Downloads</a>
          </li>
          <li rel="download-image" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
            <a rel="download-image" href="javascript:void(0);" >Download image</a>
          </li>
          <li rel="download-observed-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
            <a rel="download-observed-data" href="javascript:void(0);" >Download Observed Data</a>
          </li>
          <li rel="download-historical-modeled-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
            <a rel="download-historical-modeled-data" href="javascript:void(0);" >Download Historical Modeled Data</a>
          </li>
          <li rel="download-projected-modeled-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
            <a rel="download-projected-modeled-data" href="javascript:void(0);" >Projected Modeled Data</a>
          </li>
          <li rel="download-interpreting" icon="fas fa-download select-icon" class="default-select-option">
            <a rel="download-interpreting" href="/resources/data/Key-to-Climate-Explorer-Download-Filenames-and-Column-Headings.xlsx" >Information for Interpreting Data</a>
          </li>
        </ul>
      </div>
    </div>

  </div>

